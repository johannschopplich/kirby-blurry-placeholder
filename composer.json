{
  "name": "johannschopplich/kirby-blurry-placeholder",
  "description": "Blurry image placeholders for better UX",
  "type": "kirby-plugin",
  "version": "3.0.3",
  "keywords": [
    "kirby",
    "placeholder",
    "lazyload",
    "image",
    "seo"
  ],
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/kirby-blurry-placeholder#readme",
  "authors": [
    {
      "name": "Johann Schopplich",
      "email": "pkg@johannschopplich.com",
      "homepage": "https://johannschopplich.com"
    }
  ],
  "require": {
    "getkirby/composer-installer": "^1.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "@stable",
    "getkirby/cms": "^3.9"
  },
  "scripts": {
    "fix": "php-cs-fixer fix",
    "dist": "composer install --no-dev --optimize-autoloader"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "kirby-cms-path": false
  }
}
